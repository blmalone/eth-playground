{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es2020",
    "outDir": "./dist",
    "sourceMap": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "composite": true,
    "resolveJsonModule": true,
    "declaration": true,
    "noImplicitAny": false,
    "removeComments": true,
    "noLib": false,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "useUnknownInCatchVariables": false,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "moduleResolution": "node",
    "isolatedModules": true,
    "types": [
      "jest",
      "node"
    ],
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*",
      ]
    }
  },
  "typeRoots": [
    "node_modules/@types"
  ],
  "include": [
    "src/**/*",
    "src/op-token-price/price-history.json",
    "src/op-token-price/hello.json",
  ],
  "exclude": [
    "node_modules",
    "dist"
  ]
}
